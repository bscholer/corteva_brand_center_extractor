{
  "name": "Corteva Brand Center Extractor",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "A Chrome extension to scrape data from Corteva's brand center and export it to a CSV.",
  "author": "Ben Scholer",
  "permissions": [
    "https://brandcenter.corteva.com/*"
  ],
  "background": {
    "scripts": [
      "jquery-3.5.1.min.js",
      "papaparse.min.js",
      "index.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://brandcenter.corteva.com/*"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "papaparse.min.js",
        "index.js"
      ]
    }
  ]
}